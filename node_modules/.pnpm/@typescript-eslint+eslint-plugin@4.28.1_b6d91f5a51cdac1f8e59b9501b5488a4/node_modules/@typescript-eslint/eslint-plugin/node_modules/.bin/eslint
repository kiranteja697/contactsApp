#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules/@typescript-eslint/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/eslint@7.30.0/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules/@typescript-eslint/node_modules:/home/workspace/reactjs/sessions/on-demand-session-part-2/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.28.1_b6d91f5a51cdac1f8e59b9501b5488a4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@7.30.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@7.30.0/node_modules/eslint/bin/eslint.js" "$@"
fi
